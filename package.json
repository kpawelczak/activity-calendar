{
	"name": "activity-calendar",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"dev:ssr": "ng run activity-calendar:serve-ssr",
		"serve:ssr": "node dist/activity-calendar/server/main.js",
		"build:ssr": "ng build --prod && ng run activity-calendar:server:production",
		"firebase:deploy": "npm run build:ssr && firebase deploy",
		"prerender": "ng run activity-calendar:prerender"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~11.1.1",
		"@angular/cdk": "^11.1.2",
		"@angular/common": "~11.1.1",
		"@angular/compiler": "~11.1.1",
		"@angular/core": "~11.1.1",
		"@angular/fire": "^6.1.4",
		"@angular/forms": "~11.1.1",
		"@angular/material": "^11.1.2",
		"@angular/platform-browser": "~11.1.1",
		"@angular/platform-browser-dynamic": "~11.1.1",
		"@angular/platform-server": "~11.1.1",
		"@angular/router": "~11.1.1",
		"@angular/service-worker": "~11.1.1",
		"@nguniversal/express-engine": "^11.1.2",
		"express": "^4.15.2",
		"firebase": "^8.2.5",
		"hammerjs": "^2.0.8",
		"rxjs": "~6.6.3",
		"tslib": "^2.0.0",
		"uuid": "^8.3.2",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1101.2",
		"@angular/cli": "~11.1.2",
		"@angular/compiler-cli": "~11.1.1",
		"@angular/language-service": "~11.1.1",
		"@nguniversal/builders": "^11.1.2",
		"@types/express": "^4.17.0",
		"@types/jest": "^26.0.15",
		"@types/node": "^12.11.1",
		"codelyzer": "^6.0.0",
		"jest": "^26.6.3",
		"jest-preset-angular": "^8.3.2",
		"protractor": "~7.0.0",
		"ts-node": "~7.0.0",
		"tslint": "~6.1.0",
		"typescript": "~4.0.5"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/setupJest.ts"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/node_modules/",
			"<rootDir>/dist/"
		],
		"globals": {
			"ts-jest": {
				"tsConfig": "<rootDir>/tsconfig.spec.json",
				"stringifyContentPathRegex": "\\.html$"
			}
		}
	}
}
